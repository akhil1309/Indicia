{
    "Services": [
      {
        "Url": "http://example.com/api/v1",
        "Paging": {
          "PageSize": 5,         // Each page contains 5 items
          "OffsetType": "cursor",   // Offset are based on cursor
          "NextPageKey": "nextPageToken"
           "Parameters": {
            "Limit": "limit",     // Name of the 'limit' query parameter
            "Offset": "pageToken"      // Name of the 'offset' query parameter
          }
        },
        "Endpoints": [
          {
            "Path": "/users",
            "Method": "GET",
            "ItemType": "User",
            "Uri": "%[coveo_url]/users/%[id]",
            "ClickableUri": "%[coveo_url]/users/%[id]",
            "Title": "%[name]",
            "ModifiedDate": "%[updated]",
            "Metadata": {
              "id": "%[id]",
              "username": "%[name]"
            }
          }
        ]
      }
    ]
  }